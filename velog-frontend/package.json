    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "code-mirror-themes": "^1.0.0",
    "codemirror": "^5.42.0",
    "css-loader": "0.28.7",
    "date-fns": "^1.29.0",
    "dotenv": "4.0.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "immer": "^1.1.3",
    "immutable": "^3.8.2",
    "include-media": "^1.4.9",
    "invariant": "^2.2.3",
    "jest": "20.0.4",
    "lodash": "^4.17.5",
    "markdown-toc": "^1.2.0",
    "marked": "^0.5.2",
    "moment": "^2.22.1",
    "nanobar": "^0.4.2",
    "node-sass": "^4.9.0",
    "object-assign": "4.1.1",
    "open-color": "^1.6.1",
    "perfect-scrollbar": "^1.3.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "prismjs": "^1.10.0",
    "promise": "8.0.1",
    "query-string": "^5.0.1",
    "raf": "3.4.0",
    "react": "^16.4.1",
    "react-autosize-textarea": "^3.0.3",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-icon-base": "^2.1.2",
    "react-icons": "^2.2.7",
    "react-onclickoutside": "^6.7.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-tooltip": "^3.4.3",
    "react-truncate": "^2.4.0",
    "recompose": "^0.30.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.4.0",
    "redux-pender": "^1.2.1",
    "remarkable": "^1.7.1",
    "remove-markdown": "^0.3.0",
    "sass-loader": "^6.0.6",
    "sortablejs": "^1.7.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "finalize": "yarn build && yarn build:server && yarn deploy && yarn invalidate",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "build:server": "node scripts/build.server.js && cp ../velog-ssr/src/ssr/render.js ../velog-ssr/dist/ssr/render.js && cp ./build/asset-manifest.json ../velog-ssr/",
    "test": "node scripts/test.js --env=jsdom",
    "deploy": "aws s3 sync ./build s3://cdn.velog.io/",
    "invalidate": "aws cloudfront create-invalidation --distribution-id E1M9D68QZ0CD1F --paths / /index.html /error.html /service-worker.js /manifest.json /favicon.ico"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "flow-bin": "0.72.0",
    "onchange": "^3.3.0",
    "prettier": "^1.12.1",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "proxy": "http://localhost:4000/"
}
